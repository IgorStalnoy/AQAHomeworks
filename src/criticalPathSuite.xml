<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Critical path suite" verbose="1" parallel="methods" thread-count="2" data-provider-thread-count="2">
    <listeners>
        <listener class-name="com.aqa.homeworks.utils.TestListener"/>
    </listeners>
    <test name="Critical path test">
        <groups>
            <run>
                <include name="criticalPath"/>
            </run>
        </groups>
        <packages>
            <package name="com.aqa.homeworks.tests"/>
        </packages>
    </test>
</suite>